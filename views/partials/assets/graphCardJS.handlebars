{{!--
    // adds this graph card's data to Vizor.pageObjects.graphs{}
    // also places graph._creator data in Vizor.pageObjects.profiles{}
    // NB it is highly likely that Vizor.pageObjects is not a store yet
    // NB also see userpages.js
--}}
<script>
    (function() {
        var j = {{{json this deleteKeys="withActions,allowAllActions,withLinks,withActionEdit,withActionView,withActionChoose"}}}
        var page = window.Vizor.pageObjects
        {{#if _id}}
            page.graphs['{{_id}}'] = j
        {{else}}
            page.graphs['{{{path}}}'] = j
        {{/if}}

        {{#if _creator}}
            if (!page.profiles['{{_creator}}'])
                page.profiles['{{_creator}}'] = {}

            {{#if profile}}
                page.profiles['{{_creator}}'] = {{{json profile}}}
            {{/if}}
            page.profiles['{{_creator}}']['_id'] = j._creator
        {{/if}}

    })();
</scr{{undefined}}ipt>