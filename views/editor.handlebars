<script type="text/javascript">
	var boot = {}
	boot.hasEdits = {{hasEdits}}

	// THREE webvr-manager reads from here.
	window.Vizor = {
		hideWebVRButton: true
	}

	{{#if graph}}
		boot.graph = {
			path: '{{graph.path}}',
			url: '{{graph.url}}'
		}

		boot.graphUrl = boot.graph.url
	{{/if}}

	function startEditor(vr_devices) {
		E2.InitialiseEngi(vr_devices, boot.graphUrl)
	}

	function EnumerateVRDevices(devices) {
		var d = hardware.enumerateVRDevices(devices);
		startEditor([d.hmd, d.sensor]);
	}

	$(document).ready(function() {
		if (!Cookies.get('vizor'))
			E2.util.checkBrowser();

		E2.models = {
			user: new User({})
		}

		if ('{{user.username}}') {
			E2.models.user.set( {
				id: '{{user._id}}',
				name: '{{user.name}}',
				username: '{{user.username}}',
				email: '{{user.email}}',
			})
		}

		E2.controllers = {
			account: new AccountController()
		}

		if (!hardware.ifVR(EnumerateVRDevices)) {
			startEditor([null, null])
		}

	});
</script>
